generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Manhwa {
  id          String    @id @default(uuid()) 
  title       String    @unique @db.VarChar(250)  
  cover_image String?   @db.VarChar(250)
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt

  @@index([title])
  @@index([id])

  @@map("manhwa")
}